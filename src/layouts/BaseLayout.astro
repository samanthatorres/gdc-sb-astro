---
import Header from "../components/Header.astro";
import "../style/main.css";
import Footer from "../components/Footer.astro";
import '@fontsource/dm-serif-display/400-italic.css';
import '@fontsource/poppins/400.css';
import '@fontsource/poppins/700.css';
---

<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width" />
    <link rel="icon" type="image/x-icon" href="/favicon.svg" />
    <title>SEO Consulting: Strategy + Tech + Data | Gray Dot Co</title>

    <script defer src="https://unpkg.com/@alpinejs/ui@3.13.5-beta.0/dist/cdn.min.js"></script>
    <script defer src="https://unpkg.com/@alpinejs/focus@3.13.5/dist/cdn.min.js"></script>
    <script defer src="https://unpkg.com/alpinejs@3.13.5/dist/cdn.min.js"></script>
  </head>
  <body class="max-w-full container mx-auto">
    <Header />
    <main class="max-w-full container mx-auto">
      <slot />
    </main>
    <Footer />
  </body>
</html>
