---
import BaseLayout from "../../layouts/BaseLayout.astro";
import { getRedirects } from "../../functions/getRedirects";

const astroRedirects = await getRedirects();
---

<BaseLayout>
  <code><pre>{
    Object.entries(astroRedirects).map(([url, dest]) => `${url} => ${dest}\n`)
  }
  </code></pre>
</BaseLayout>
